{
  "name": "skyward",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm run gen:version && rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "compile": "rm -rf ./dist && tsc -p ./tsconfig.json",
    "test": "tsx ../../node_modules/uvu/bin.js . .+\\.test\\.ts",
    "test:watch": "watchlist --eager src -- pnpm run test",
    "prepublishOnly": "pnpm run build",
    "gen:version": "genversion --es6 src/version.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    "./prepare": {
      "types": "./dist/prepare.d.ts",
      "import": "./dist/prepare.js",
      "default": "./dist/prepare.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "default": "./dist/browser.js"
    }
  },
  "files": [
    "dist/",
    "bin/"
  ],
  "bin": {
    "skyward": "bin/skyward.js"
  },
  "dependencies": {
    "@inquirer/input": "^2.0.1",
    "@inquirer/password": "^2.0.1",
    "@noble/hashes": "^1.3.3",
    "@ts-rest/core": "^3.33.0",
    "cachedir": "^2.4.0",
    "cli-table3": "^0.6.3",
    "codeco": "^1.2.1",
    "filesize": "^10.1.0",
    "formdata-node": "^6.0.3",
    "multiformats": "^13.1.0",
    "p-defer": "^4.0.0",
    "sade": "^1.8.1",
    "serve-handler": "^6.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/serve-handler": "^6.1.4",
    "@types/tmp": "^0.2.6",
    "genversion": "^3.2.0",
    "o1js": "^0.16.2",
    "tmp-promise": "^3.0.3"
  },
  "peerDependencies": {
    "o1js": "^0.16.2"
  }
}
